  "scripts": {
    "develop": "node ./scripts/app-start.js",
    "dev": "next dev",
    "build": "next build",
    "postbuild": "next-sitemap",
    "start": "next start -p 80",
    "start:p:t": "node ./scripts/app-start-prod.js",
    "lint": "next lint",
    "lint:fix": "f() { if [ $npm_config_registry = $npm_config_npm_IF_NPM ]; then npm run lint -- --fix; else yarn lint --fix; fi; }; f",
    "prepare": "husky install",
    "pre-commit": "lint-staged",
    "check-env": "node -e 'console.log(process.env)'",
    "test": "jest --passWithNoTests",
    "codegen": "cross-env DOTENV_CONFIG_PATH='./.env.development' graphql-codegen -r dotenv/config",
    "codegen:query": "graphql-codegen --config codegen.watch.yml",
    "codegen:watch": "graphql-codegen --config codegen.watch.yml --watch",
    "collect-translations": "i18next 'src/**/*.{ts,tsx}' --config ./scripts/next-i18next-parser.config.js",
    "clt": "$npm_execpath collect-translations",
    "analyze": "ANALYZE=true next build",
    "sequelize-autogen": "node ./scripts/sequelize-auto.js"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": [
      "yarn lint"
    ]
  },
